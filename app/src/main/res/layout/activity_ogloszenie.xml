<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <FrameLayout
        android:id="@+id/frameStackdescription"
        style="@style/groupStylecornerRadius_1"
        android:layout_width="@dimen/_330pxh"
        android:layout_height="@dimen/_150pxv">

        <TextView
            android:id="@+id/txtDescription"
            style="@style/txtInterbold14"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="@dimen/_17pxh"
            android:layout_marginTop="@dimen/_19pxv"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginBottom="@dimen/_19pxv"
            android:gravity="top|start"
            android:text="@{homeViewVM.homeViewModel.txtDescription}"
            android:textAllCaps="true"
            android:textColor="#A1887F"
            tools:ignore="TextSizeCheck"
            tools:text="@string/msg_nocna_jazda_sta" />

        <View
            android:id="@+id/viewRectangleSix"
            android:layout_width="@dimen/_330pxh"
            android:layout_height="@dimen/_150pxv"
            android:layout_gravity="start"
            android:background="@drawable/rectangle_bg_white_a700_26_radius_30" />
    </FrameLayout>


</androidx.constraintlayout.widget.ConstraintLayout>